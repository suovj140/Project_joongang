<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTDMapper3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.OrderMapper">

    <!-- 해당 유저의 주문목록 모두 가져오기 -->
    <select id="getUserOrderList" resultType="com.example.demo.vo.OrderVO" parameterType="String">
        select * from tbl_order where user_email = #{user_email}
    </select>

</mapper>